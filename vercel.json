{
    "version": 2,
     "builds":[
         {
             "src":"./index.js",
             "use":"@vercel/node"
         }
     ],
     "routes":[
         {
             "src":"/(.*)",
             "dest":"/index.js"
         }
 
     ],
     "functions": {
    "path/to/dir/**/*": {
      "maxDuration": 5 // All functions can run for a maximum of 5 seconds
    }
  }
 }
